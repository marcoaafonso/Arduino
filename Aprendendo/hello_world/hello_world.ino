void setup() {
  // Define o pino 15 como saida
  pinMode(15, OUTPUT);
}

void loop() {
  digitalWrite(15, HIGH); // Acende o Led
  delay(1000); // Aguarda 1 segundo
  digitalWrite(15, LOW); // Apaga o Led
  delay(1000); // Aguarda 1 segundo

digitalWrite(15, HIGH); // Acende o Led
  delay(1000); // Aguarda 1 segundo
  digitalWrite(15, LOW); // Apaga o Led
  delay(1000); // Aguarda 1 segundo

digitalWrite(15, HIGH); // Acende o Led
  delay(1000); // Aguarda 1 segundo
  digitalWrite(15, LOW); // Apaga o Led
  delay(1000); // Aguarda 1 segundo

  digitalWrite(15, HIGH); // Acende o Led
  delay(100); // Aguarda 1 segundo
  digitalWrite(15, LOW); // Apaga o Led
  delay(100); // Aguarda 1 segundo

digitalWrite(15, HIGH); // Acende o Led
  delay(100); // Aguarda 1 segundo
  digitalWrite(15, LOW); // Apaga o Led
  delay(100); // Aguarda 1 segundo

digitalWrite(15, HIGH); // Acende o Led
  delay(100); // Aguarda 1 segundo
  digitalWrite(15, LOW); // Apaga o Led
  delay(1000); // Aguarda 1 segundo
}
